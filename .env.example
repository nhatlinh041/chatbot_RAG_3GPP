# 3GPP RAG System Environment Variables
# Copy this file to .env and fill in your values

# Claude API key (required for Claude model)
CLAUDE_API_KEY=your-anthropic-api-key-here

# Neo4j connection (optional, defaults shown)
NEO4J_URI=neo4j://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Local LLM endpoint (optional, for Ollama)
LOCAL_LLM_URL=http://192.168.1.14:11434/api/chat
